#include <vector>
#include <iostream>
#include <string>
#include <algorithm>
using namespace std;


int main()
{
	for (int a = 0; a < 5; a++)
	{
		int input;
		cout << "Input a charater to display it in ASCII\n";
		cin >> input;
		cout << input << " has ASCII code " << static_cast<char>(input) << endl;
	}
	return 0;
}
